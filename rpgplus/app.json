{
  "expo": {
    "name": "RPGPlus",
    "slug": "rpgplus",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ff5700"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rpgplusteam.rpgplus"
    },
    "android": {
      "package": "com.rpgplusteam.rpgplus",
      "versionCode": 1,
      "permissions": [
        "CAMERA_ROLL"
      ]
    }
  }
}
